re()\n\n    const submit = async values => {\n      await store.dispatch('request/create', values)\n      emit('created')\n    }\n\n    return {\n      ...useRequestForm(submit)\n    }\n  }\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""}]}