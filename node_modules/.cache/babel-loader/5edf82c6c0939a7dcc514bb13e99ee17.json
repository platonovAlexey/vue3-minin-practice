ap = {\n      active: 'primary',\n      cancelled: 'danger',\n      done: 'primary',\n      pending: 'warning'\n    }\n\n    const textMap = {\n      active: 'Активен',\n      cancelled: 'Отменен',\n      done: 'Завершен',\n      pending: 'Выполняется'\n    }\n\n    watch(props, val => {\n      className.value = classesMap[val.type]\n      text.value = textMap[val.type]\n    })\n\n    const className = ref(classesMap[props.type])\n    const text = ref(textMap[props.type])\n\n    return {\n      className,\n      text\n    }\n  }\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""}]}